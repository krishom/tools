{
  "name": "Just for U Coupon Clicker",
  "version": "1.0",
  "permissions": [
    "activeTab"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["http://www.safeway.com/ShopStores/*"],
      "css": ["content_script.css"],
      "js": ["content_script.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click All Coupons!"
  },
  "manifest_version": 2
}